@misc{Iivari2004,
abstract = {This paper introduces the idea of coding a practically relevant body of knowledge (BoK) in Information Systems (IS) that could have major benefits for the field. In its main part, the paper focuses on the question if and how an underlying body of action-oriented knowledge for IS experts could be distilled from the IS research literature. For this purpose the paper identifies five knowledge areas as the most important parts for an IS expert's BoK. Two of these are claimed as distinct areas of competence for IS experts: IS application knowledge and IS development (ISD) process knowledge. The paper focuses particularly on ISD process knowledge because it allows the organizing of practically relevant IS knowledge in an action-oriented way. The paper presents some evidence for the claim that a considerable body of practically relevant IS process knowledge might, indeed, exist, but also notes that it is highly dispersed in the IS literature. It then argues that the IS research community should take stock of this knowledge and organize it in an action-oriented way. Based on results from prior work it proposes a four-level hierarchical coding scheme for this purpose. In order to test the idea of coding action-oriented knowledge for IS experts, the paper reports the results of a coded literature analysis of ISD research articles published from 1996 to 2000 in two leading IS journals – Information Systems Journal and MIS Quarterly. The results suggest that ISD approaches form a useful framework for organizing practically relevant IS knowledge.},
author = {Iivari, Juhani and Hirschheim, Rudy and Klein, Heinz K.},
booktitle = {Information Systems Journal},
doi = {10.1111/j.1365-2575.2004.00177.x},
isbn = {1365-2575},
issn = {13501917},
keywords = {Competence,Expert,Information system,Professionalization},
number = {4},
pages = {313--342},
pmid = {13667},
title = {{Towards a distinctive body of knowledge for information systems experts: Coding ISD process knowledge in two IS journals}},
volume = {14},
year = {2004}
}
