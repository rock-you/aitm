@inproceedings{Hayes1999,
abstract = {The Department of Defense, like other organizations, is making wide use of internal and external information that is publicly available on the World Wide Web. The World Wide Web Consortium (W3C) has recently developed the specification for an eXtensible Markup Language (XML) that enables the packaging of data and its metadata in a platform-independent, application-neutral manner. XML will enable search engines to be more discriminating (via metadata-based filter) when processing a search request. Additionally, XML provides a structure for applications to filter and manipulate information to directly meet the user's needs. These two capabilities will revolutionize information processing; but for this to occur, information providers must describe their data and information requesters must describe their requests in terms that are commonly shared (common metadata, multiple forms) within some defined community. The authors anticipate that XML will cause a metadata explosion to occur in the near future due to two basic causes: the ease in creating and packaging metadata with data coupled with the pent-up demand for techniques to improve searching for information resources. The widespread and growing awareness of metadata as a means to efficiently identify and locate resources is giving us a window of opportunity to invest in an infrastructure that can deliver these services. However, the agreements within some identified community of interest that are manifest in a common representation (community logical model) can not be allowed to become a relic sitting on a shelf. The common representation must become an active component in information retrieval.   This paper describes a strategy for managing, publicizing, sharing, and using metadata to identify and retrieve information resources within the Department of Defense that incorporates the use of XML as a data and metadata interchange format. The tools and refinements to current specifications and policy that will be necessary to build the infrastructure and make it used to support this vision are also described.},
address = {Bethesda, Maryland},
annote = {Some commentary on the early impact of MOF on military systems. Also references XIF.},
author = {Hayes, Glenda and Daniels, Robert M},
booktitle = {META-DATA '99  THIRD IEEE META-DATA CONFERENCE},
keywords = {MOF},
title = {{The Next Desktop Revolution: End-user Metadata}},
year = {1999}
}
